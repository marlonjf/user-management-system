<header>
  <div class="navbar navbar-default">
    <div class="container">
      <%= link_to root_path, class:"navbar-header" do %>
        <%#= image_tag "ambev/logo_claro.png", class: 'logo' %>
      <% end %>
      <%# if admin_signed_in? %>
        <div>
          <%#= link_to (company_signed_in? ? "Logout admin" : "Logout"), destroy_admin_session_path, data: { method: :delete } %>
          <%#= link_to "Logout empresa", destroy_company_session_path, data: { method: :delete }, class: 'pl-sm-4' if company_signed_in? %>
        </div>
      <%# elsif company_signed_in? %>
        <%#= link_to "Logout", destroy_company_session_path, data: { method: :delete } %>
      <%# end %>
    </div>
  </div>
</header>
